<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ieco">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_ieco"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_ieco_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IEco"
        tools:targetApi="31">
        <activity
            android:name=".Activities.ViewArticleActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.BrowseArticlesActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.ViewEventActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.CalendarActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.EditFaqsActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.EditArticleActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.EditEventActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.MainActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.AdminActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.AuthenticationActivity"
            android:exported="false" />
    </application>

</manifest>